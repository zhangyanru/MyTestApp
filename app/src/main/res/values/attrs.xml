<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ColorChangedCircleView">
        <attr name="firstColor" format="color"/>
        <attr name="secondColor" format="color"/>
        <attr name="speed" format="integer"/>
        <attr name="circleWidth" format="dimension"/>
    </declare-styleable>
    <declare-styleable name="RoundProgressBar">
        <attr name="p_negativeColor" format="color"/>
        <attr name="p_positiveColor" format="color"/>
        <attr name="p_ringWidth" format="dimension"/>
        <attr name="p_textColor" format="color"/>
        <attr name="p_textSize" format="dimension"/>
        <attr name="p_progress" format="dimension"/>
    </declare-styleable>

    <attr name="position">
        <enum name="left_top" value="0" />
        <enum name="right_top" value="1" />
        <enum name="right_bottom" value="2" />
        <enum name="left_bottom" value="3" />
    </attr>
    <declare-styleable name="ArcMenu">
        <attr name="position" />
        <attr name="arc_r" format="dimension"/>
    </declare-styleable>

    <declare-styleable name="ArcMenu2">
        <attr name="arc2_r" format="dimension"/>
        <attr name="position2">
            <enum name="left_top" value="0"/>
            <enum name="left_bottom" value="1"/>
            <enum name="right_top" value="2"/>
            <enum name="right_bottom" value="3"/>
        </attr>
        <attr name="pointId" format="integer"/>
        <attr name="isPointById" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="ArcMenu3">
        <attr name="arc3_r" format="dimension"/>
        <attr name="position_arc3">
            <enum name="left_top_arc3" value="0"/>
            <enum name="left_bottom_arc3" value="1"/>
            <enum name="right_top_arc3" value="2"/>
            <enum name="right_bottom_arc3" value="3"/>
        </attr>
        <attr name="pointId3" format="integer"/>
        <attr name="isPointById3" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="CustomSlideDeleteListView">
        <attr name="rightViewId" format="integer"/>
    </declare-styleable>

    <declare-styleable name="CustomSlideDeleteItemView">
        <attr name="contentViewId" format="integer"/>
    </declare-styleable>

    <declare-styleable name="CustomPullToZoomListView">
        <attr name="headerlayout" format="reference"/>
        <attr name="zoomLayout" format="reference"/>
    </declare-styleable>

    <declare-styleable name="CustomExpandableLayout">
        <attr name="contentLayout" format="reference"/>
        <attr name="headLayout" format="reference"/>
    </declare-styleable>

    <declare-styleable name="CustomExpandableLayoutItem">
        <attr name="itemLayout" format="reference"/>
        <attr name="menuLayout" format="reference"/>
    </declare-styleable>

    <declare-styleable name="PullToZoomView">
        <attr name="headerView" format="reference" />
        <attr name="contentView" format="reference" />
        <attr name="zoomView" format="reference" />
        <attr name="isHeaderParallax" format="boolean" />
    </declare-styleable>

    <declare-styleable name="SlidingTabLayout">
        <attr name="indicatorColor" format="color"/>
        <attr name="shouldExpand" format="boolean" />
        <attr name="tabbackground" format="reference" />
        <attr name="tabBackgroundTextViewId" format="reference"/>
    </declare-styleable>

</resources>